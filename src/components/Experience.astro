---
import ExperienceItem from './ExperienceItem.astro'

const experiences = [
  {
    title: 'Web Developer',
    description:
      'Lead developer for multiple high-traffic web applications, for a larger clients in United States.',
    company: 'BEST Websites',
    duration: 'January 2024 - August 2024',
    place: 'Remote'
  },
  {
    title: 'Front-end web Developer',
    description:
      'Developed and maintained web applications for a larger clients in United States.',
    company: 'Titan Digital',
